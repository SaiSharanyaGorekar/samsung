			hexToRgb: function(hex) {
				try {
					if(hex[0] == '#') hex = hex.substr(1);
					
					var bigint = parseInt(hex, 16);
					var r = (bigint >> 16) & 255;
					var g = (bigint >> 8) & 255;
					var b = bigint & 255;
					
					return {r: r, g: g, b: b};
				}
				catch(e) {
					return {r: 0, g: 0, b: 0};
				}
			},
			mixHex: function(hex0, hex1, weight) {
