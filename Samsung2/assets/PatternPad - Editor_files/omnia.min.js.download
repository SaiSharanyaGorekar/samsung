						} else {
							// potentially powered by InAppBrowser because that (currently) clobbers window.open
							log('omnia.tools.browser.openBrowser: not available, url = \'' + args.url + '\'');
							window.open(args.url, '_blank', 'location=yes');
						}
					});
				}
				catch(e) {
