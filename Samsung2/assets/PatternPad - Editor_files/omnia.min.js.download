					this.t0.setDate(this.t0.getDate() + days);
					if(this.t1) this.t1.setDate(this.t1.getDate() + days);
				} else {
					this.t0.setTime(this.t0.getTime() + ms);
					if(this.t1) this.t1.setTime(this.t1.getTime() + ms);
				}
				return this;
			},
