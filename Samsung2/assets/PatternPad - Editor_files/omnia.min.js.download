			rgbToHex: function(r, g, b) {
				try {
					var dec2hex = function(rgb) { 
						var hex = Number(rgb).toString(16);
						if(hex.length < 2) {
							hex = "0" + hex;
						}
						return hex;
					};
					
					return dec2hex(r) + dec2hex(g) + dec2hex(b);
				}
				catch(e) {
					return '000000';
				} 
			},
			hexToLuma: function(hex) {
