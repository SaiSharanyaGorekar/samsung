				if(!task || !task.redo) return;
				
				task.redo(task);
				
				log('omnia.tools.Undo.redo: idx = ' + (this.pointer + 1) + ' / ' + this.stack.length);
				if(this.onChange) this.onChange(this.getStack());
			},
