			maps: function(query, provider) {
				var self = omnia.tools.client.maps;
				
				if(provider == true && ox.pf.os != 'android') {
					ox.ov.show({
						content: {oid: 'bar2', css: 'ox-context ox-large', c: [
							{oid: 'label', text: query},
