								case 'margin-left': 
									var val = typeof val == 'object' && val['data-value'] ? val['data-value'] : val;
									elem.css('margin-left', val);
									break;
								case 'margin-right': 
									var val = typeof val == 'object' && val['data-value'] ? val['data-value'] : val;
									elem.css('margin-right', val);
									break;
								case 'margin-top': 
									var val = typeof val == 'object' && val['data-value'] ? val['data-value'] : val;
									elem.css('margin-top', val);
									break;
								case 'margin-bottom': 
