							ox.pf.os == 'android' ? null : {oid: 'button', css: 'ox-id-close', label: 'Apple', click: () => { self(query, 'apple'); }},
							{oid: 'button', css: 'ox-id-close', label: 'Google', click: () => { self(query, 'google'); }},
