					if(idx !== null && idx >= 0) {
						if(!isRemove) ox.arr.insertAt(row, idx, itemI);
						else ox.arr.removeAt(row, idx);
					} else {
						row.push(itemI);
					}
				});
				
				return ox.copy(array);
