							oid: 'movieitemsmall',
							image: item.Poster ? {style: {'background-image': 'url(' + item.Poster + ')'}} : null,
							headline: item.Title,
							time: item.Year,
							css: 'ox-id-track-oe-t hand pb15 pl15',
							style: {'width': (100 / Math.floor(me.list.width() / 150)) + '%'},
							click: function() { me.close(item); },
